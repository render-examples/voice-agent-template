# LiveKit Cloud API keys (Server-side only)
# Run `lk cloud auth && lk app env -w` to automatically populate these values
LIVEKIT_API_KEY=your_api_key_here
LIVEKIT_API_SECRET=your_api_secret_here
LIVEKIT_URL=wss://your-project.livekit.cloud

# Rime API key (Server-side only)
# Get your API key from https://rime.ai/
RIME_API_KEY=your_rime_api_key_here

# Next.js Public Variables (accessible in browser)
# This should be the same as LIVEKIT_URL above
NEXT_PUBLIC_LIVEKIT_URL=wss://your-project.livekit.cloud
